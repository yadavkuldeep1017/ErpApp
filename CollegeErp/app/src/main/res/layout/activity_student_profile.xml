<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/admbg2"
    tools:context=".StudentProfile">

    <include
        android:id="@+id/include"
        layout="@layout/toolbar"
        android:layout_width="match_parent"
        android:layout_height="71dp"
        android:layout_marginTop="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.cardview.widget.CardView
        android:layout_width="350dp"
        android:layout_height="600dp"

        android:layout_below="@+id/include"
        android:layout_centerInParent="true"
        android:layout_marginTop="20dp"
        android:backgroundTint="@color/white"
        app:cardCornerRadius="25dp"
        app:cardElevation="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="63dp"
                android:gravity="center"
                android:padding="10dp"
                android:text="My Profile"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="bold|italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="105dp"
                android:layout_height="120dp"
                android:layout_marginTop="68dp"
                android:src="@drawable/user"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView8" />

            <TableLayout
                android:layout_width="348dp"
                android:layout_height="410dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="1dp"
                android:layout_marginEnd="1dp"
                android:layout_marginBottom="1dp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView7">

                <TableRow
                    android:id="@+id/row1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="9dp">

                    <TextView
                        android:id="@+id/name1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Name:-"

                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>

                    <TextView
                        android:id="@+id/name2"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="Kuldeep Yadav"
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="9dp">

                    <TextView
                        android:id="@+id/course1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Course:-    "
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>

                    <TextView
                        android:id="@+id/course2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="MCA"
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="9dp">

                    <TextView
                        android:id="@+id/prno1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="PRN:-    "
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>

                    <TextView
                        android:id="@+id/prno2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1111"
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="9dp">

                    <TextView
                        android:id="@+id/rollno1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Roll No:-    "
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>

                    <TextView
                        android:id="@+id/rollno2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1111"
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="9dp">

                    <TextView
                        android:id="@+id/year1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Study Year:-    "
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>

                    <TextView
                        android:id="@+id/year2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="F.Y"
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="9dp">

                    <TextView
                        android:id="@+id/gmail1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Gmail Id:-    "
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>

                    <TextView
                        android:id="@+id/gmail2"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:text="dssd"
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="9dp">

                    <TextView
                        android:id="@+id/gender1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Gender:-    "
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>

                    <TextView
                        android:id="@+id/gender2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Male"
                        android:textColor="@color/black"
                        android:textSize="20sp"></TextView>
                </TableRow>

            </TableLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>